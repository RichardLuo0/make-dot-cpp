{
  "name": "makeDotCpp",
  "packages": [
    "std",
    "boost",
    {
      "path": "packages/glob.json"
    }
  ],
  "dev": {
    "debug": true,
    "packages": [
      "std",
      "makeDotCpp"
    ]
  },
  "usage": {
    "compileOption": "-fprebuilt-module-path={0}/lib",
